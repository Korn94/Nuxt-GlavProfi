{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host=0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "migrate": "npx drizzle-kit --config=drizzle.config.ts migrate",
    "migrate:generate": "npx drizzle-kit --config=drizzle.config.ts generate",
    "migrate:push": "npx drizzle-kit --config=drizzle.config.ts push",
    "migrate:all": "npm run migrate:generate && npm run migrate"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "^7.4.3",
    "animate.css": "^4.1.1",
    "bcryptjs": "^3.0.2",
    "devtools": "^8.42.0",
    "echarts": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "node-cron": "^4.2.0",
    "nuxt": "^4.0.3",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@iconify-json/fa6-solid": "^1.2.4",
    "@iconify-json/fluent": "^1.2.29",
    "@iconify-json/iconamoon": "^1.2.2",
    "@iconify-json/solar": "^1.2.4",
    "@nuxt/icon": "^2.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.4",
    "mysql2": "^3.14.1",
